{
  "defaultSeverity": "error",
  "jsRules": {},
  "rules": {
    "prettier": true,
    "no-console": true,
    "object-literal-sort-keys": false,
    "no-var-keyword": true,
    "no-submodule-imports": [true, "next"],
    "prefer-const": true,
    "ordered-imports": false,
    "interface-name": false,
    "member-access": false,
    "jsx-no-lambda": false
  },
  "radix": true,
  "rulesDirectory": ["tslint-plugin-prettier", ".prettierrc"],
  "extends": [
    "tslint:latest",
    "tslint-react",
    "tslint-eslint-rules",
    "tslint-config-prettier"
  ]
}
