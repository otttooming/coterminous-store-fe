  },
  "dependencies": {
    "babel-plugin-styled-components": "1.5.1",
    "classnames": "2.2.6",
    "coterminous-styled": "git+https://eecac4a4af30c56755b8f5fde964782b490ecc32:x-oauth-basic@github.com/otttooming/coterminous-styled.git",
    "downshift": "^1.31.16",
    "express": "4.16.3",
    "front-matter": "2.3.0",
    "isomorphic-unfetch": "2.0.0",
    "localforage": "1.7.1",
    "next": "6.0.3",
    "next-redux-wrapper": "2.0.0-beta.6",
    "react": "16.4.0",
    "react-dom": "16.4.0",
    "react-markdown": "3.3.2",
    "react-paginate": "5.2.3",
    "react-photoswipe": "1.3.0",
    "react-popper": "^1.0.0-beta.6",
    "react-redux": "5.0.7",
    "redux": "4.0.0",
    "redux-devtools-extension": "2.13.2",
    "redux-thunk": "2.3.0",
    "styled-components": "3.3.2"
  },
  "devDependencies": {
    "@types/babel-core": "6.25.5",
    "@types/classnames": "2.2.4",
    "@types/express": "4.16.0",
    "@types/jest": "23.0.2",
    "@types/next": "2.4.12",
    "@types/next-redux-wrapper": "1.4.1",
    "@types/node-sass": "3.10.32",
    "@types/photoswipe": "^4.0.28",
    "@types/prettier": "1.13.1",
    "@types/react": "16.3.17",
    "@types/react-autosuggest": "9.3.4",
    "@types/react-dom": "16.0.6",
    "@types/react-paginate": "4.3.2",
    "@types/react-redux": "6.0.2",
    "@types/whatwg-fetch": "0.0.33",
    "@zeit/next-typescript": "1.0.1",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-jest": "23.0.1",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2017": "6.24.1",
    "babel-preset-react": "6.24.1",
    "jest": "23.1.0",
    "jest-cli": "23.1.0",
    "node-sass": "4.9.0",
    "nodemon": "^1.17.5",
    "photoswipe": "^4.1.2",
    "prettier": "1.13.5",
    "ts-jest": "22.4.6",
    "ts-loader": "4.4.1",
    "ts-node": "^6.1.0",
    "tslint": "5.10.0",
    "tslint-config-airbnb": "5.9.2",
    "tslint-config-prettier": "1.13.0",
    "tslint-loader": "3.6.0",
    "tslint-plugin-prettier": "1.3.0",
    "tslint-react": "3.6.0",
    "typescript": "2.9.1"
  },
  "scripts": {
    "dev": "npm run get:omniva && npm run get:smartpost && nodemon server/index.ts",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "test": "jest --no-cache",
    "test:watch": "jest --watch",
    "sprite": "svg-sprite-generate -d ./static/assets -o ./static/sprite.svg",
    "style": "node-sass styles/main.scss static/css/styles.css",
    "get:omniva": "curl https://www.omniva.ee/locations.json -o ./static/shipping/omniva.json",
    "get:smartpost": "curl http://uus.smartpost.ee/places.json -o ./static/shipping/smartpost.json"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "verbose": true,
    "bail": false,
    "collectCoverage": true
  },
  "author": "Ott Tooming",
  "license": "ISC"
}
